<Window x:Class="WPFDungeon.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFDungeon"
        mc:Ignorable="d" Closing="Window_Closing"
        Title="RestartWindow" Height="400" Width="300">
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" HorizontalAlignment="Right">
            <Button Content="LogIn" Height="20" Width="60" Click="LogIn_Click"/>
            <Button Content="Register" Height="20" Width="60" Click="Register_Click"/>
        </StackPanel>
        <Frame x:Name="frame"
               Height="350" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
